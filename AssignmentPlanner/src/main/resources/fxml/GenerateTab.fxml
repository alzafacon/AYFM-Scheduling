<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane fx:id="generateTab" 
            minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"
            xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" 
            fx:controller="io.fidelcoria.ayfmPlanner.controller.GenerateTabController">
  <children>
    <ChoiceBox fx:id="scheduleMonthChoiceBox" disable="false" layoutX="14.0" layoutY="14.0" prefWidth="100.0">
    </ChoiceBox>
    <ChoiceBox id="" fx:id="scheduleYearChoiceBox" layoutX="211.0" layoutY="14.0" prefWidth="50.0">
    </ChoiceBox>
    <Button fx:id="generateScheduleButton" 
            disable="false" layoutX="382.0" layoutY="14.0" mnemonicParsing="false" 
            onAction="#generateSchedule" text="generate schedule" />
    
    <Button id="" fx:id="chooseScheduleButton" layoutX="14.0" layoutY="100.0" mnemonicParsing="false" onAction="#openScheduleFilePicker" text="choose schedule" />
    <Label id="" fx:id="scheduleChosenForRemindersLabel" layoutX="211.0" layoutY="100.0" text="no file chosen" />
    <Button id="" fx:id="generateRemindersButton" 
            disable="false" layoutX="372.0" layoutY="102.0" mnemonicParsing="false" 
            onAction="#generateReminders" text="generate reminders" />
  </children>
</AnchorPane>
